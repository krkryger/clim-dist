    "## PCA\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn import preprocessing\n",
    "\n",
    "shuffled_patterns = cleaned_patterns\n",
    "np.random.shuffle(shuffled_patterns)\n",
    "\n",
    "df_data = {entry['pattern']: list(model[entry['pattern']]) for entry in shuffled_patterns[:100]}\n",
    "df = pd.DataFrame(df_data).transpose()\n",
    "df.head()\n",
    "\n",
    "# from: https://www.youtube.com/watch?v=Lsue2gEM9D0\n",
    "\n",
    "pca = PCA()\n",
    "pca.fit(df)\n",
    "pca_data = pca.transform(df)\n",
    "\n",
    "per_var = np.round(pca.explained_variance_ratio_* 100, decimals=1)\n",
    "labels = ['PC' + str(x) for x in range(1, len(per_var)+1)]\n",
    "\n",
    "plt.figure(figsize=(18,6))\n",
    "plt.bar(x=range(1,len(per_var)+1), height=per_var, tick_label=labels)\n",
    "plt.ylabel('Percentage of Explained Variance')\n",
    "plt.xlabel('Principal Component')\n",
    "plt.title('Scree Plot')\n",
    "plt.show()\n",
    "\n",
    "pca_df = pd.DataFrame(data=pca_data, index=df.index, columns=labels)\n",
    "\n",
    "plt.figure(figsize=(18,18))\n",
    "\n",
    "plt.scatter(pca_df.PC1, pca_df.PC2)\n",
    "plt.title('Pattern words in vector space')\n",
    "plt.xlabel('PC1 - {0}%'.format(per_var[0]))\n",
    "plt.ylabel('PC2 - {0}%'.format(per_var[1]))\n",
    " \n",
    "for sample in pca_df.index:\n",
    "    plt.annotate(sample, (pca_df.PC1.loc[sample], pca_df.PC2.loc[sample]), size=12)\n",
    "    \n",
    "plt.show()"
